  dc:issued "2015-01-09T09:28:37+0000" ;
  dc:modified "2015-01-16T14:16:27+0000" ;
  dc:publisher <http://www.antwerpen.be> ;
  dc:source <http://geodata.antwerpen.be/arcgissql/rest/services/P_Publiek/OpenDataAntwerpen/MapServer/115> ;
  dc:date "2015-01-16T15:16:25+0100" ;
  dc:language <http://lexvo.org/id/iso639-3/nld> ;
  dcat:distribution <http://datasets.antwerpen.be/v4/public/gis/bpacontour.json> .

