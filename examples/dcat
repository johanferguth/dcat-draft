  dc:issued "2014-12-03T14:27:36+0000" ;
  dc:modified "2015-01-16T14:15:48+0000" ;
  dc:publisher <http://www.antwerpen.be> ;
  dc:source <http://geodata.antwerpen.be/arcgissql/rest/services/P_Publiek/OpenDataAntwerpen/MapServer/75> ;
  dc:date "2015-01-16T15:15:46+0100" ;
  dc:language <http://lexvo.org/id/iso639-3/nld> ;
  dcat:distribution <http://datasets.antwerpen.be/v4/public/gis/rsaalternatievetramlijn.json> .

